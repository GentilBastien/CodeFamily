{
  "singleQuote": true,
  "trailingComma": "es5",
  "printWidth": 120,
  "endOfLine": "auto",
  "arrowParens": "avoid",
  "bracketSpacing": true,
  "htmlWhitespaceSensitivity": "css",
  "bracketSameLine": false,
  "quoteProps": "as-needed",
  "tabWidth": 2,
  "useTabs": false,
  "requirePragma": false,
  "semi": true,
  "singleAttributePerLine": false,
  "overrides": [
    {
      "files": "*.html",
      "options": {
        "printWidth": 120
      }
    }
  ]
}
